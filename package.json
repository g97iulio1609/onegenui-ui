{
  "name": "@onegenui/ui",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "Core UI primitives for OneGenUI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./definitions": {
      "types": "./dist/definitions.d.ts",
      "import": "./dist/definitions.mjs",
      "require": "./dist/definitions.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@onegenui/core": "workspace:*",
    "@onegenui/react": "workspace:*",
    "@onegenui/schemas": "workspace:^",
    "@onegenui/utils": "workspace:^",
    "clsx": "^2.1.1",
    "framer-motion": "^12.26.2",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.1",
    "lightweight-charts": "^5.0.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^2.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.9.2",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "clsx": "^2.0.0",
    "lightweight-charts": "^5.0.0",
    "lucide-react": "^0.562.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.0.0",
    "zod": "^4.0.0"
  }
}